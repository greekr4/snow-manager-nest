  duration     Int?
  userAgent    String?  @db.Text
  ip           String?
  timestamp    DateTime @default(now())

  @@index([timestamp], map: "ApiLog_timestamp_idx")
  @@index([userId], map: "ApiLog_userId_idx")
}

